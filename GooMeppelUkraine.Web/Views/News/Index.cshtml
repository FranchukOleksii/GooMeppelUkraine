@model List<GooMeppelUkraine.Web.Models.Article>

<h2>News</h2>

<ul class="list-group">
    @foreach (var a in Model)
    {
        <li class="list-group-item">
            <a asp-action="Details" asp-route-id="@a.Id">@a.Title</a>
            <div class="text-muted small">@a.CreatedAtUtc.ToString("yyyy-MM-dd HH:mm")</div>
        </li>
    }
</ul>
